/*
--SEQUENCE
CREATE SEQUENCE SEQUENCE_NAME
[INCREMENT BY N] <-시퀀스 번호 증가값 : 기본 값 1
[START WITH N] <-시퀀스 시작 번호 : 기본 값 1
[MAXVALUE N | NOMAXVALUE] <- SEQUENCE 최댓값
[MINVALUE N | NOMINVALUE] <- CYCLE 옵션 활성시 시작값
[CYCLE | NOCYCLE] <-반복 여부 결정 [최대/최소 이후]
[CACHE N | NOCACHE] <- 생성된 SEQUENCE 메모리에 CACHING
[ORDER | NOORDER] <-요청순서 보장 여부(RAC)
*/

BEGIN
FOR I IN 1..11 LOOP
    INSERT INTO DEPT_E
    VALUES(SEQ_DEPT_E1.NEXTVAL, DBMS_RANDOM.STRING('A', 4));
END LOOP;
    COMMIT;
END;
/
SELECT * FROM USER_SEQUENCES WHERE SEQUENCE_NAME = 'SEQ_DEPT_E';  //시퀀스 검색 하기
