select * from 테이블이름;
select * from 테이블이름 where 조건부;
select distinct 칼럼이름 from 테이블이름;  //칼럼이름에 있는 인자들 중복없이 출력
select * from 테이블이름 order by 칼럼이름; //칼럼이름에 있는 인자들을 오름차순으로 정렬
select * from 테이블이름 order by 칼럼이름 desc; //칼럼이름에 있는 인자들을 내림차순으로 정렬

-----------------고급----------------------

select 칼럼이름1,칼럼이름2,... from 테이블이름 where 칼럼이름 like 'c%';    //원하는 칼럼이름이 c로 시작하는 칼럼이름1,칼럼이름2,... 출력



*ALTER

alter table 테이블이름 change 칼럼이름 바꾸고싶은이름 구조; //필드명 변경
alter table 테이블이름 drop 칼럼이름;                  //칼럼지우기
alter table 테이블이름 add 칼럼이름 구조;               //칼럼 추가
alter table 테이블이름 modify 칼럼이름 구조;            //칼럼 변경
alter table 테이블이름 rename 바꾸고싶은이름;           //테이블명 변경
